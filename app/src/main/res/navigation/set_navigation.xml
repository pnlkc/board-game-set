<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/set_navigation"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/setFragment"
        android:name="com.example.set.SetFragment"
        android:label="SetFragment" >
        <action
            android:id="@+id/action_setFragment_to_endFragment"
            app:destination="@id/endFragment" />
    </fragment>
    <fragment
        android:id="@+id/startFragment"
        android:name="com.example.set.StartFragment"
        android:label="start_fragment"
        tools:layout="@layout/start_fragment" >
        <action
            android:id="@+id/action_startFragment_to_setFragment"
            app:destination="@id/setFragment" />
    </fragment>
    <fragment
        android:id="@+id/endFragment"
        android:name="com.example.set.EndFragment"
        android:label="EndFragment" >
        <action
            android:id="@+id/action_endFragment_to_setFragment"
            app:destination="@id/setFragment"
            app:popUpTo="@id/setFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>