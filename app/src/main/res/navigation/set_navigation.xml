<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/set_navigation"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.example.set.StartFragment"
        android:label="start_fragment"
        tools:layout="@layout/start_fragment" >
        <action
            android:id="@+id/action_startFragment_to_setFragment"
            app:destination="@id/setFragment" />
        <action
            android:id="@+id/action_startFragment_to_setRuleFragment"
            app:destination="@id/setRuleFragment" />
    </fragment>
    <fragment
        android:id="@+id/setFragment"
        android:name="com.example.set.SetFragment"
        android:label="set_fragment"
        tools:layout="@layout/set_fragment" >
        <action
            android:id="@+id/action_setFragment_to_startFragment"
            app:destination="@id/startFragment" />
    </fragment>
    <fragment
        android:id="@+id/setRuleFragment"
        android:name="com.example.set.SetRuleFragment"
        android:label="setrule_fragment"
        tools:layout="@layout/setrule_fragment" >
        <action
            android:id="@+id/action_setRuleFragment_to_setFragment"
            app:destination="@id/setFragment" />
        <action
            android:id="@+id/action_setRuleFragment_to_startFragment"
            app:destination="@id/startFragment" />
    </fragment>
</navigation>